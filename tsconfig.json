{
  "compilerOptions": {
    "target": "es2015",
    "outDir": "spec-out/",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "removeComments": true,
    "sourceMap": true,
    "newLine": "LF"
  },
  "exclude": [
    "node_modules",
    "custom_typings",
    "coverage",
    "spec-out",
    "dist",
    "scripts",
    "src/__test__",
    "src/lib/__test__"
  ]
}
